// Online C compiler to run C program online
#include<stdio.h>
int main()
{
    int m,n,r,rev=0;
    printf("enter a number");
    scanf("%d",&n);
    m=n;
    while(n>0)
    {
    r=n%10;
    rev=(rev*10)+r;
    n=n/10;
    }
    if(m==rev)
    printf("%d is a palindrome ",m);
    else
    printf("%d is not a palindrome",m);
    return 0;
}
