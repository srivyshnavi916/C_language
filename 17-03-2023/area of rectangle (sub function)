#include <stdio.h>

// Function to calculate area of rectangle
int area(int length, int width) {
    return length * width;
}

int main() {
    int l, w;

    printf("Enter length and width of rectangle: ");
    scanf("%d %d", &l, &w);

    int a = area(l, w);

    printf("The area of the rectangle is: %d\n", a);

    return 0;
}
